public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int temp = n;    
        int reverse = 0;

        while (temp != 0) {
            int digit = temp % 10;
            reverse = reverse * 10 + digit;
            temp = temp / 10;
        }
        if(n==reverse){
        System.out.println(n+ " palindrome ");
        }
        else{
            System.out.println(n+ " not ");
        }

        System.out.println( + reverse);
    }
}
