import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int temp = n;    
        int sum = 0;
        while (temp != 0) {
            int digit = temp % 10;
        sum = sum + (digit* digit * digit);
            temp = temp / 10;
        }
        if(sum==n){
        System.out.println(n+ " Armstrong number ");
        }
        else{
            System.out.println(n+ " not Armstrong number ");
        }
    }
}
